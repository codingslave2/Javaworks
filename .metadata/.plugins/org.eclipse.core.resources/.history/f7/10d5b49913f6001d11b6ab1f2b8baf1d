package hiding;

public class Account {

	 // 맴버 변수, 생성자, 매소드
	
	private String acc; // 계좌번호
	private String owner; // 계좌주
	private int balance; // 잔고
	
	public Account(String acc, String owner, int balance) {
		this.acc = acc;
		this.owner = owner;
		this.balance = balance;
	}
	
	// get() 메소드 : get + 맴버 변수 이름()
	
	public String getAcc() {
		return acc;
	}
	
}
