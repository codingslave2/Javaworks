package ch03;

public class TypeConversion {

	public static void main(String[] args) {
		 // 형 변환
		 // 묵시적 형변형(자동 형변형)
		int iNum = 20;
		float fNum = iNum;
		
		System.out.println(iNum); // 20
		System.out.println(fNum); // 20.0
		
		double dNum;
		dNum = iNum + fNum;
		
		System.out.println(dNum);
	}

}
