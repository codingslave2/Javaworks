package methods;

public class OneUp {
	
	// 숫자 1 증가 함수
	public static int oneUp() {
		int x = 0;
		x = x + 1;
		return x;
		
	}
	
	
	
	

	public static void main(String[] args) {

		System.out.println(oneUp());
		System.out.println(oneUp());
		System.out.println(oneUp());
		System.out.println(oneUp());
		System.out.println(oneUp());
		
		
	}

}
