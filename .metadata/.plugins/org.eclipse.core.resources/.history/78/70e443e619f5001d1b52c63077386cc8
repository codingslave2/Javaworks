package ch03;

public class BitTest2 {

	public static void main(String[] args) {
		// 비트 연산자 논리
		int num1 = 5; // 00000101
		int num2 = 10; // 00001010
		int result1, result2;
		
		// 비트 연산
		result1 = num1 & num2;
		result2 = num1 | num2;
		
		System.out.println(result1);
		System.out.println(result2);
		
		
		
	}

}
