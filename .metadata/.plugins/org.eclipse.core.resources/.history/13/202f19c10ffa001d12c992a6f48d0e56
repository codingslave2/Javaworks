package polymorphism;

class Animal{
	public void move() {
		System.out.println("동물이 움직입니다.");
	}
	
}

// Human 클래스 
class Human extends Animal{
	public void move() {
		System.out.println("사람이 움직입니다.");
	}



public class AnimalTest {

	public static void main(String[] args) {
		//
		
		Animal animal = new Animal();
		animal.move();
	}

}
