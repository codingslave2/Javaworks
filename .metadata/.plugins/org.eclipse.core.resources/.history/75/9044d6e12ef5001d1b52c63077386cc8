package methods;

public class Math1 {

	public static void main(String[] args) {
		// Math 클래스
		// 절대값 구하는 함수 사용 - abs()
		// new 클래스는 클래스 이름으로 직접 접근
		
		int v1 = Math.abs(-7);
		System.out.println("v1 = " + v1);
		
		// 반올림
		long v2 = Math.round(5.7);
		System.out.println("v2 = " + v2);
		
		
		
	}

}
