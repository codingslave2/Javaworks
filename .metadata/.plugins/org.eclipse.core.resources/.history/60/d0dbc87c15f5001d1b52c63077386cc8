package ch03;

public class TypeConversion {

	public static void main(String[] args) {
		 // 형 변환
		 // 묵시적 형변형(자동 형변형)
		int iNum = 20;
		float fNum = iNum; // 4바이트 실수형
		
		System.out.println(iNum); // 20
		System.out.println(fNum); // 20.0
		
		double dNum;
		dNum = iNum + fNum;
		
		System.out.println(dNum); // 40.0
		
		// 명시적 형변환(강제 형변환)
		double dNum1 = 1.2;
		float fNum2 = 0.9F;
	}

}
