package objectArray;

public class BookMain {

	public static void main(String[] args) {
		// Book 객체 생성 = 책 5권
		
		Book[] library = new Book[5];
		
		//System.out.println(library[0]);
		// 배열의 자료가 없는 경우 -- null
		
		
		for(int i=0; i<library.length; i++) {
			System.out.println(library[i]);
		}
		
		
	}

}
