package twodimention;

public class TwoDimension {

	public static void main(String[] args) {
		// 문자형 2차원 배열
		char[][] alphabets = new char [13][2];
		char ch = 'A';
		
		// 알파벳 저장
		for(int i=0; i<alphabets.length; i++) {
			for(int j=0; j<alphabets.length; j++) {
				alphabets[i][j] = ch;
				ch++''
			}
		}
		
	}

}
